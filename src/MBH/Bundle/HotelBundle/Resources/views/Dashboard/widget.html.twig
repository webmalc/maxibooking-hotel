<div class="col-md-6" id="{{ id }}">
    <a target="_blank" href="{{ path(routeName) }}" class="small-box">
        <div class="inner">
            <h3>{{ title|trans }}</h3>
            <h3 class="flow-progress text-center">&nbsp;</h3>
        </div>
        <div class="icon" style="color: white;">
            <i class="fa fa-{{ icon }}"></i>
        </div>
        <div class="small-box-footer">
            {{ 'views.dashboard.index.widgets.configure'|trans }} <i class="fa fa-arrow-circle-right"></i>
        </div>
    </a>
</div>