{% extends "MBHHotelBundle::layout.html.twig" %}

{% set management_icon = true %}
{% set title = 'views.hotel.layout.hotels'|trans({}, 'MBHHotelBundle') %}
{% set title_url = path('room_type') %}

{% block scripts %}
    {{ parent() }}

    {% javascripts filter='?uglifyjs2'
        '@MBHHotelBundle/Resources/public/js/room-type/*'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

{% block styles %}
    {{ parent() }}

    {% stylesheets filter='cssrewrite, uglifycss' '@MBHHotelBundle/Resources/public/css/room-type/*'%}
        <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}