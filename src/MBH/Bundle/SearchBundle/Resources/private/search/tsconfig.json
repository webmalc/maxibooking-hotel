{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "outDir": "/build/",
    "strict": false,
    "lib": ["es2015", "es2015.iterable", "es2016", "dom"],
    "experimentalDecorators": true,
    "types": ["node", "jquery", "select2", "bootstrap"],
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules"
  ]
}