{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span class="icon">{#<img src="" alt=""/>#}</span>
        <span class="sf-toolbar-status">SearchCache</span>
        <span class="sf-toolbar-value">D-{{ collector.DoctrineCacheItems }}:R-{{ collector.RedisCacheItems }}</span>
    {% endset %}
    {#{% set text %}#}
        {##}
    {#{% endset %}#}


    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig') }}
{% endblock %}

    {% block menu %}
        {# This left-hand menu appears when using the full-screen profiler. #}
        <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt=""/></span>
        <strong>CacheResult</strong>
            <span class="count"><span>{{ collector.DoctrineCacheItems }}</span><span>{{ collector.RedisCacheItems }}</span></span>
    </span>
    {% endblock %}


    {% block panel %}
         Optional, for showing the most details.
        <h2>Redis - Doctrine cache items</h2>
        <table>
            <tr>
                <th>Current Cache Items</th>
            </tr>

            <tr>
                <td>DoctrineCache-{{ collector.DoctrineCacheItems }}</td>
            </tr>
            <tr>
                <td>RedisCache-{{ collector.RedisCacheItems }}</td>
            </tr>
        </table>
    {% endblock %}
