{% extends "MBHBaseBundle::page.html.twig" %}

{% block scripts %}
    {{ parent() }}
    {% javascripts filter='uglifyjs2''@MBHPriceBundle/Resources/public/js/*'%}

    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

{% block styles %}
    {{ parent() }}

    {% stylesheets filter='cssrewrite, uglifycss' '@MBHPriceBundle/Resources/public/css/*'%}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}

{% set management_icon = true %}
{% set title = 'menu.label.nomandprice'|trans({},'MBHBaseBundle') %}