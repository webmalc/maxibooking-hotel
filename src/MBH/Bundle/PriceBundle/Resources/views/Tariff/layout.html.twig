{% extends "MBHPriceBundle::layout.html.twig" %}

{% set small_title = 'mbhpricebundle.view.tariff.layout.tarify'|trans %}
{% set title_url = path('tariff') %}

{% block scripts %}
    {{ parent() }}
    {% javascripts filter='uglifyjs2''@MBHPriceBundle/Resources/public/js/tariff/*'%}

        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

{% block styles %}
    {{ parent() }}

    {% stylesheets filter='cssrewrite, uglifycss' '@MBHPriceBundle/Resources/public/css/tariff/*'%}
        <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}