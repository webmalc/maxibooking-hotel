<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Maxibooking</title>
    {% set isYandexAnalyticEnabled = formConfig and formConfig.yandexAnalyticConfig and formConfig.yandexAnalyticConfig.isEnabled == true %}
    {% if isYandexAnalyticEnabled %}
        <!-- Yandex.Metrika counter -->
        <script type="text/javascript" >
            var counterId = "{{ formConfig.yandexAnalyticConfig.id }}";
            var counterObjName = 'yaCounter' + counterId;
            (function (d, w, c) {
                (w[c] = w[c] || []).push(function() {
                    try {
                        w[counterObjName] = new Ya.Metrika({
                            id: counterId,
                            clickmap:true,
                            trackLinks:true,
                            accurateTrackBounce:true,
                            webvisor:true
                        });
                    } catch(e) { }
                });

                var n = d.getElementsByTagName("script")[0],
                    s = d.createElement("script"),
                    f = function () { n.parentNode.insertBefore(s, n); };
                s.type = "text/javascript";
                s.async = true;
                s.src = "https://mc.yandex.ru/metrika/watch.js";

                if (w.opera == "[object Opera]") {
                    d.addEventListener("DOMContentLoaded", f, false);
                } else { f(); }
            })(document, window, "yandex_metrika_callbacks");
        </script>
        <noscript><div><img src="https://mc.yandex.ru/watch/48663167" style="position:absolute; left:-9999px;" alt="" /></div></noscript>
        <!-- /Yandex.Metrika counter -->
    {% endif %}
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.css">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/select2/3.5.0/select2.css">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/fancybox/3.0.47/jquery.fancybox.css">
    {% if formConfig and formConfig.theme %}
        <link rel="stylesheet" href="{{ formConfig.theme }}">
    {% endif %}
</head>

<body>
    <div id="mbh-results-wrapper" style=" width: auto; height: auto; padding: 10px 20px; overflow: auto;"></div>
    {% if formConfig and formConfig.css %}
        <style>
         {{ formConfig.css }}
        </style>
    {% endif %}
    {% if formConfig.js %}
        <script>
            window.onload = function () {
                {{ formConfig.js|raw }}
            };
        </script>
    {% endif %}
    {#<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>#}
    <script src="//code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/select2/3.5.0/select2.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery.maskedinput/1.3.1/jquery.maskedinput.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/fancybox/3.0.47/jquery.fancybox.min.js"></script>
    <script type="text/javascript" src="{{ url('online_form_results', {id: formId}) }}"></script>
</body>

</html>
