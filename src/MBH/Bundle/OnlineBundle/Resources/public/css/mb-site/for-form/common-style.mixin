@size-screen-mid-width-tablet: 832px;
@size-screen-max-width-mobile: 732px;

@size-screen-mid-width-tablet-minus-one: 831px;
@size-screen-max-width-mobile-minus-one: 731px;

@general_color: var(--main);
@general_color_light: var(--mainlight);

.commonStyleFont() {
  font-family: 'Roboto', sans-serif;

  font-style: normal;
  font-weight: normal;
  line-height: 16px;
  font-size: 12px;
  color: #282828;
}

.fontRobotoSlab() {
  font-family: 'Roboto Slab', serif;
};

.iframeBorderStyle() {
  border-color: rgb(170, 170, 170);
  border-style: solid;
  border-width: 1px;
}

.textUppercase() {
  text-transform: uppercase;
  letter-spacing: 0.05em;
}

.widthWithCalc() {
  width: ~"calc(100vw - 30px)";
}

.fontSizeLineHeight(@_font-size: 16px, @_line-height: 24px) {
  font-size: @_font-size;
  line-height: @_line-height;
}

.maxOuterBorders(@_width_for_result_page: @size-screen-mid-width-tablet) {
  box-sizing: border-box;

  @media (min-width: 700px) {
    .widthWithCalc();
  }

  @media (min-width: @size-screen-max-width-mobile) {
    width: @size-screen-max-width-mobile;
  }

  @media (min-width: @size-screen-mid-width-tablet) {
    width: @_width_for_result_page;
  }

  width: 100vw;
}

@indent_btn: 30px;

.replaceStyleBootstrap() {
  .btn {
    .textUppercase();
  }

  .bg-primary {
    background-color: @general_color_light;
  }

  .text-primary {
    color: @general_color;
  }

  .btn-primary {
    background-color: @general_color;
    border-color: @general_color;
  }

  .btn-default {
    border-color: unset;
    background: #BDBDBD;
  }

  @media (min-width: @size-screen-max-width-mobile) {
    .panel-heading {
      padding: 15px 25px;
    }
  }

  .panel {
    box-shadow: none;
  }
}

.filterBrightnessOnHover() {
  &:hover {
    filter: brightness(120%);
  }
}
