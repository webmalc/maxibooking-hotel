{% extends "MBHBaseBundle::page.html.twig" %}

{% block scripts %}
    {{ parent() }}

    <script src="https://code.highcharts.com/highcharts.js"></script>

    {% javascripts filter='uglifyjs2'
     '@MBHPackageBundle/Resources/public/js/*'
     '@MBHCashBundle/Resources/public/js/cash.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% block customJs %}
    {% endblock %}
    {% include '@MBHPackage/Report/graphModal.html.twig' %}
{% endblock %}

{% block styles %}
    {{ parent() }}
    
    {% stylesheets filter='cssrewrite, uglifycss' '@MBHPackageBundle/Resources/public/css/*'%}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}