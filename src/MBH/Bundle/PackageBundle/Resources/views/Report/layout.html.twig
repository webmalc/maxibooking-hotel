{% extends "MBHPackageBundle::layout.html.twig" %}

{% block scripts %}
    {{ parent() }}
    {% javascripts filter='?uglifyjs2'
        '@MBHPackageBundle/Resources/public/js/report/*'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block styles %}
    {{ parent() }}

    {% stylesheets filter='cssrewrite, uglifycss' '@MBHPackageBundle/Resources/public/css/report/*'%}
        <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}

{% endblock %}