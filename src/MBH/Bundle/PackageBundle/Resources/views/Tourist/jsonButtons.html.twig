{% spaceless %}
    <div class='btn-list'>
        <a href='{{ path('tourist_edit', { 'id': entity.id }) }}' class='btn btn-success btn-xs' title='{{ 'tourist.json_buttons.edit_button.title'|trans }}' data-toggle='tooltip'>
            <i class='fa fa-pencil-square-o'></i>
        </a>
        <a href='{{ path('tourist_delete', { 'id': entity.id}) }}' class='btn btn-danger btn-xs delete-link ' title='{{ 'tourist.json_buttons.remove_button.title'|trans }}' data-toggle='tooltip'>
            <i class='fa fa-trash-o'></i>
        </a>
    </div>
{% endspaceless %}