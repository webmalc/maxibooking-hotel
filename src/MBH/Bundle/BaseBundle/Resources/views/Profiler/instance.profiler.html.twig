{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span class="icon"><img
                    src="data:image/svg+xml;utf8;base64,"/></span>
        <span class="sf-toolbar-value">{{ collector.user }}</span>
    {% endset %}

    {% set text %}
        {% if collector.dirs is iterable %}
            {% for dirname, dir in collector.dirs %}
                <div class="sf-toolbar-info-piece">
                    <br><b>{{ dirname }}</b>
                    <span>{{ dir }}</span>
                </div>
            {% endfor %}
        {% endif %}

    {% endset %}


    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { link: true }) }}
{% endblock toolbar %}

{% block head %}
    {{ parent() }}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/svg+xml;utf8;base64," alt=""/></span>
        <strong>Instance Variables</strong>
    </span>
{% endblock %}

{% block panel %}
    {# Optional, for showing the most details. #}
    <h2>Instance variables</h2>
    <table>
        <tr>
            <th>Instance dirs</th>
        </tr>

        {% for dir in collector.dirs %}
            <tr>
                <td>{{ dir }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}