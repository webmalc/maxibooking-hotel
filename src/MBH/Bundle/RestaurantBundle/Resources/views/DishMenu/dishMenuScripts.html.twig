<script>
    var ingredients = {
        {% for ingredient in ingredients %}
        '{{ ingredient.id }}' : {
            'price':{{ ingredient.price }},
            'units': '{{ (ingredient.calcType)|trans() }}',
            'currency': '{{ currency()['text']|trans }}',
            'costprice': '{{ ingredient.costPrice }}'
        },
        {% endfor %}
        'dishMenuItem': {
            'id': '{{ entry.id }}',
            'price': {{ entry.price }},
            'marginprice': {{ entry.margin }}
        }

    };
</script>